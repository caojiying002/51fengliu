<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:viewBindingIgnore="true">

    <com.jiyingcao.a51fengliu.ui.widget.StatefulLayout
        android:id="@+id/stateful_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:loadingLayout="@layout/stateful_default_loading"
        app:contentLayout="@layout/smart_refresh_recycler_view"
        app:errorLayout="@layout/stateful_default_error"
        app:emptyLayout="@layout/stateful_default_empty">
    </com.jiyingcao.a51fengliu.ui.widget.StatefulLayout>
</FrameLayout>